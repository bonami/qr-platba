{
	"name": "zoldia/qr-platba",
	"type": "library",
	"description": "Generování QR plateb v PHP.",
	"keywords": [
		"php",
		"qr",
		"platba"
	],
	"homepage": "https://github.com/zoldia/QRPlatba",
	"license": "MIT",
	"authors": [
		{
			"name": "Dennis Fridrich",
			"email": "fridrich.dennis@gmail.com"
		}
	],
	"require": {
		"php": ">=7.1",
		"endroid/qr-code": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"Defr\\QRPlatba\\": "src/"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5.5"
	},
	"config": {
		"bin-dir": "bin"
	},
	"scripts": {
		"psr-fix": [
			"php-cs-fixer fix"
		],
		"test": [
			"./bin/phpunit"
		]
	}
}
